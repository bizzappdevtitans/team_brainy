<odoo noupdate="1">

    <record id="stage_new" model="school.addmission.stage">
        <field name="name">Requested</field>
        <field name="sequence">10</field>
        <field name="state">new</field>
    </record>

    <record id="stage_open" model="school.addmission.stage">
        <field name="name">Review</field>
        <field name="sequence">20</field>
        <field name="state">open</field>
    </record>

    <record id="stage_done" model="school.addmission.stage">
        <field name="name">Confirm</field>
        <field name="sequence">90</field>
        <field name="state">done</field>
    </record>

</odoo>
